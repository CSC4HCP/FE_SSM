{
  "welcomeFile": "/index.html",
  "sendWelcomeFileRedirect": true,
  "routes": [
    {
      "path": "/resources",
      "target": {
        "type": "application",
        "name": "sapui5preview",
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "version": "snapshot",
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "application",
        "name": "sapui5preview",
        "entryPath": "/test-resources"
      },
      "description": "SAPUI5 Test Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources"
      },
      "description": "SAPUI5 Test Resources"
    },
    {
      "path": "/services/userapi",
      "target": {
        "type": "service",
        "name": "userapi"
      }
    },
    {
      "path": "/destinations/SSM_DEST",
      "target": {
        "type": "destination",
        "name": "SSM_DEST"
      },
      "description": "SSM Backend Service"
    }
  ],
  "securityConstraints": [
    {
      "permission": "AccessApplicationDescriptor",
      "description": "Access application descriptor",
      "protectedPaths": [
        "/neo-app.json"
      ]
    }
  ],
  "cacheControl": [
    {
      "directive": "private",
      "maxAge": 0,
      "path": "*html"
    },
    {
      "directive": "private",
      "maxAge": 0,
      "path": "sap-ui-cachebuster-info.json"
    },
    {
      "directive": "public",
      "maxAge": 31536000
    }
  ]
}